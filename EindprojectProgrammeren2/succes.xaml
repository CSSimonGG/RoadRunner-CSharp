<Window x:Class="EindprojectProgrammeren2.succes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EindprojectProgrammeren2"
        mc:Ignorable="d"
        Title="succes" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Assets/Hurray.png"/>
        </Grid.Background>

        <TextBlock x:Name="tbWin" Height="60" VerticalAlignment="Bottom" HorizontalAlignment="Center" FontSize="30" FontWeight="black" Text="You have won the game"></TextBlock>
    </Grid>
</Window>
